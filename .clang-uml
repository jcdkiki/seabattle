# Change to directory where compile_commands.json is
compilation_database_dir: uml-build/
# Change to directory where diagram should be written
output_directory: diagrams
diagrams:
  example_class_diagram:
    type: class
    glob:
      - src/*.cpp
    using_namespace:
      - seabattle
    include:
      namespaces:
        - seabattle
    exclude:
      namespaces:
        - seabattle::detail
